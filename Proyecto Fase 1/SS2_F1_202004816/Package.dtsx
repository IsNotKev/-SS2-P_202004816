                        string query = "INSERT INTO " + tablaVenta + " (fecha, codcliente, nombrecliente, tipocliente, direccioncliente, numerocliente, codvendedor, nombrevendedor, vacacionista, codproducto, nombreproducto, marcaproducto, categoria, sodsucursal, nombresucursal, direccionsucursal, region, departamento, unidades, preciounitario) VALUES " +
                                       "(@Fecha, @Codcliente, @Nombrecliente, @Tipocliente, @Direccioncliente, @Numerocliente, @Codvendedor, @Nombrevendedor, @Vacacionista, @Codproducto, @Nombreproducto, @Marcaproducto, @Categoria, @Sodsucursal, @Nombresucursal, @Direccionsucursal, @Region, @Departamento, @Unidades, @Preciounitario)";

                        using (SqlCommand myCommand = new SqlCommand(query, myADONETConnection))
                        {
                            myCommand.Parameters.AddWithValue("@Fecha", campos.Length > 0 ? campos[0] : "");
