                            myCommand.Parameters.AddWithValue("@Nombrecliente", campos.Length > 2 ? campos[2] : "");
                            myCommand.Parameters.AddWithValue("@Tipocliente", campos.Length > 3 ? campos[3] : "");
                            myCommand.Parameters.AddWithValue("@Direccioncliente", campos.Length > 4 ? campos[4] : "");
                            myCommand.Parameters.AddWithValue("@Numerocliente", campos.Length > 5 ? campos[5] : "");
